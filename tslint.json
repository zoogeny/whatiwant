{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "no-empty": [true, "allow-empty-functions"],
        "arrow-parens": [true, "ban-single-arg-parens"],
        "interface-over-type-literal": false
    },
    "rulesDirectory": []
}