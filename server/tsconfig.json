{
    "compilerOptions": {
        "outDir": "./dist",
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "esModuleInterop": true,
        "jsx": "react",
        "noUnusedLocals": true,
        "allowJs": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "importHelpers": true,
        "declaration": false,
        "resolveJsonModule": true,
        "target": "es5",
    }
}